{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "web-client": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "aot": false,
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/sass/styles.scss"
            ],
            "scripts": [
              "src/assets/web/assets/jquery/jquery.min.js",
              "src/assets/popper/popper.min.js",
              "src/assets/bootstrap/js/bootstrap.min.js",
              "src/assets/smoothscroll/smooth-scroll.js",
              "src/assets/dropdown/js/nav-dropdown.js",
              "src/assets/dropdown/js/navbar-dropdown.js",
              "src/assets/touchswipe/jquery.touch-swipe.min.js",
              "src/assets/tether/tether.min.js",
              "src/assets/ytplayer/jquery.mb.ytplayer.min.js",
              "src/assets/vimeoplayer/jquery.mb.vimeo_player.js",
              "src/assets/masonry/masonry.pkgd.min.js",
              "src/assets/viewportchecker/jquery.viewportchecker.js",
              "src/assets/slidervideo/script.js",
              "src/assets/imagesloaded/imagesloaded.pkgd.min.js",
              "src/assets/bootstrapcarouselswipe/bootstrap-carousel-swipe.js",
              "src/assets/theme/js/script.js",
              "src/assets/gallery/script.js",
              "node_modules/my-application-mpp-lib/kotlin/my-application-mpp-lib.js"
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "web-client:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "web-client:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "web-client:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.scss"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "tsconfig.app.json",
              "tsconfig.spec.json",
              "e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "web-client:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "web-client:serve:production"
            }
          }
        }
      }
    }
  },
  "defaultProject": "web-client"
}
